{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm install && npx turbo run build",
  "installCommand": "echo 'Skip'",
  "rewrites": [
    {
      "source": "/taviti/:path*",
      "destination": "https://taviti-web.vercel.app/:path*"
    },
    {
      "source": "/ilaia/:path*",
      "destination": "https://ilaia-web.vercel.app/:path*"
    }
  ]
}
